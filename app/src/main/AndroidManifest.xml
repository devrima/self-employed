<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.devrim.selfemployed">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.AddProjectActivityActivity"
            android:label="@string/add_project_activity_label"></activity>
        <activity
            android:name=".activity.AddProjectActivity"
            android:label="@string/add_project_label" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ReviewProjectActivity"
            android:label="@string/review_project_label"/>
    </application>
</manifest>
